<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #D2B4DE;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.loose.fis.sre.controllers.FAQController">
    <children>
        <HBox layoutX="15.0" layoutY="10.0" prefHeight="34.0" prefWidth="585.0">
            <children>
                <Button fx:id="backButtonQ" mnemonicParsing="false" onAction="#handleBackButtonQAction" prefWidth="100.0" style="-fx-background-color: #4A235A; -fx-background-insets: 0; -fx-background-radius: 20;" text="Back" textFill="WHITE">
                    <font>
                        <Font name="System Bold Italic" size="12.0" />
                    </font></Button>
            </children>
        </HBox>
      <TableView fx:id="questionTable" layoutX="35.0" layoutY="100.0" prefHeight="250.0" prefWidth="250.0">
        <columns>
          <TableColumn fx:id="idQColumn" prefWidth="125.0" text="IdQ" />
          <TableColumn fx:id="questionsColumn" prefWidth="125.0" text="Questions" />
        </columns>
      </TableView>
      <TableView fx:id="answerTable" layoutX="316.0" layoutY="100.0" prefHeight="250.0" prefWidth="250.0">
        <columns>
          <TableColumn fx:id="idAColumn" prefWidth="125.0" text="IdQ" />
          <TableColumn fx:id="answerColumn" prefWidth="125.0" text="Answers" />
        </columns>
      </TableView>


    </children>
</AnchorPane>
